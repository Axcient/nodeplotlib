{
  "name": "nodeplot",
  "version": "0.0.1",
  "description": "NodeJS plot lib on top of plotly.js",
  "main": "index.js",
  "scripts": {
    "test": "istanbul cover node_modules/mocha/bin/_mocha test/*.js -- -R spec",
    "uglify": "uglifyjs dist/index.js --mangle --compress -o dist/index.min.js",
    "build": "tsc -p src && npm run uglify"
  },
  "keywords": [
    "node",
    "plot",
    "plotting",
    "plotly",
    "plotlyjs",
    "easy-to-use",
    "backend"
  ],
  "author": "Felix Lemke <flemke.dev@gmail.com> (https://felixlemke.com)",
  "license": "MIT",
  "dependencies": {
    "plotly.js": "^1.41.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.5",
    "@types/mocha": "^5.2.5",
    "chai": "^4.1.2",
    "coveralls": "^3.0.2",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "source-map-support": "^0.5.9",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.3",
    "uglify-es": "^3.3.9"
  }
}
