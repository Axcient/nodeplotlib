{
  "name": "nodeplotlib",
  "version": "0.2.11",
  "description": "NodeJS plot lib on top of plotly.js for easy serverside plot spawns",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngfelixl/nodeplotlib.git"
  },
  "bugs": {
    "url": "https://github.com/ngfelixl/nodeplotlib/issues"
  },
  "homepage": "https://github.com/ngfelixl/nodeplotlib#readme",
  "scripts": {
    "test": "istanbul cover node_modules/mocha/bin/_mocha test/*.js -- -R spec",
    "uglify": "uglifyjs dist/index.js --mangle --compress -o dist/index.min.js",
    "copy": "copyfiles -u 1 src/www/index.html dist",
    "build": "tsc -p src/www && tsc -p src && npm run copy && npm run uglify"
  },
  "keywords": [
    "plot",
    "plotting",
    "plotlyjs",
    "science",
    "publication-ready",
    "statistics",
    "easy-to-use",
    "no-frontend-prep",
    "browser",
    "localhost"
  ],
  "author": "Felix Lemke <flemke.dev@gmail.com> (https://felixlemke.com)",
  "license": "MIT",
  "dependencies": {
    "express": "^4.16.3",
    "opn": "^5.3.0",
    "plotly.js": "^1.41.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.5",
    "@types/express": "^4.16.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.10.1",
    "@types/opn": "^5.1.0",
    "@types/plotly.js": "^1.38.8",
    "chai": "^4.1.2",
    "copyfiles": "^2.1.0",
    "coveralls": "^3.0.2",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "source-map-support": "^0.5.9",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3",
    "uglify-es": "^3.3.9"
  }
}
